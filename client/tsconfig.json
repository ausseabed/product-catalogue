{
  "extends": "@quasar/app/tsconfig-preset",
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "include": [
    "src/**/*",
    "*.js"
  ],
  "exclude": [
    ".quasar",
    "node_modules",
    "dist"
  ]
}